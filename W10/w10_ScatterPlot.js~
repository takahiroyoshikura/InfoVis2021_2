class ScatterPlot {
	constructor( config, data ) {
		this.config = {
			parent: config.parent,
			width: config.width || 256,
			height: config.height || 256,
			margin: config.margin || {top:10, right:10, bottom:10, left:10},
			title: config.title || '',
			xlabel: config.xlabel || '',
			ylabel: config.ylabel || ''
		}
		this.data = data;
		this.init();
	}
